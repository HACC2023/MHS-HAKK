generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Coordinates {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  Latitude  Float
  Longitude Float
  address   String
}

model Data {
  id                          String @id @default(auto()) @map("_id") @db.ObjectId
  ACCEPTING_NEW_PATIENTS      String @map("ACCEPTING NEW PATIENTS")
  ADDITIONAL_INFO_FOR_PAGE    String @map("ADDITIONAL INFO FOR PAGE")
  ADDRESS                     String
  DOCTOR_NAME                 String @map("DOCTOR NAME")
  DOCTOR_NUMBER               String @map("DOCTOR NUMBER")
  HEALTH_CENTER_NAME          String @map("HEALTH CENTER NAME")
  HOSPITAL_NUMBER             String @map("HOSPITAL NUMBER")
  HOSPITAL_WEBSITE            String @map("HOSPITAL WEBSITE")
  INSURANCE_PLAN              String @map("INSURANCE PLAN")
  NON_ENGLISH_LANGUAGE_SPOKEN String @map("NON-ENGLISH LANGUAGE SPOKEN")
  SERVICE_NAME                String @map("SERVICE NAME")
}

model HealthCenter {
  id                  String   @id @default(auto()) @map("_id") @db.ObjectId
  address             String
  healthCenterNumbers String[]
  insurancePlans      String[]
  names               String[]
  website             String?
}

model Post {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model ProcedureReview {
  id              String  @id @default(auto()) @map("_id") @db.ObjectId
  covered         Boolean
  hadQuest        Boolean
  healthCenterID  String  @db.ObjectId
  name            String
  procedureTypeID String  @db.ObjectId
  reviewID        String  @db.ObjectId
  type            String
}

model ProcedureType {
  id                   String   @id @default(auto()) @map("_id") @db.ObjectId
  acceptingNewPatients Boolean?
  doctorNumbers        String[]
  healthCenterID       String   @db.ObjectId
  insurancePlan        String
  name                 String
  nonEnglishLanguages  String[]
  reviewIDs            String[]
}

model Review {
  id               String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt        DateTime @db.Date
  hadQuest         Boolean
  healthCenterID   String   @db.ObjectId
  procedureTypeIDs String[]
}

